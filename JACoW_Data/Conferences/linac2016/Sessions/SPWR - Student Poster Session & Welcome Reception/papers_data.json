    {
      "paper_id": "SPWR025",
      "title": "Measurement of the Transverse Beam Dynamics in a TESLA-type Superconducting Cavity",
      "authors": [
        "A. Halavanau",
        "P. Piot",
        "Northern Illinois University",
        "DeKalb",
        "Illinois",
        "USA",
        "N. Eddy",
